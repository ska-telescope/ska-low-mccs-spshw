{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://skao.int/MccsTile_SetVoltageWarningThresholds.json",
    "title": "MccsTile SetVoltageWarningThresholds schema",
    "description": "Schema for MccsTile's SetVoltageWarningThresholds command",
    "type": "object",
    "properties": {
        "voltage": {
            "description": "Name of the voltage to set the warning thresholds for",
            "type": "string"
        },
        "min_thr": {
            "description": "Threshold minimum value",
            "type": "number",
            "minimum": 0.0
        },
        "max_thr": {
            "description": "Threshold maximum value",
            "type": "number"
        }
    },
    "required": [
        "voltage",
        "min_thr",
        "max_thr"
    ]
}