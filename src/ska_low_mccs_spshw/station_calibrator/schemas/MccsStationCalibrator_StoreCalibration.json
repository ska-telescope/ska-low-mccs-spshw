{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://skao.int/MccsStationCalibrator_StoreCalibration.json",
    "title": "MccsStationCalibrator StoreCalibration schema",
    "description": "Schema for MccsStationCalibrator's StoreCalibration command",
    "type": "object",
    "properties": {
        "frequency_channel": {
            "description": "Freqency channel to store a solution for",
            "type": "integer",
            "minimum": 0,
            "maximum": 511
        },
        "solution": {
            "description": "The calibration solution to store",
            "type": "array",
            "items": {
                "type": "number"
            }
        }
    },
    "required": ["frequency_channel", "solution"]
}